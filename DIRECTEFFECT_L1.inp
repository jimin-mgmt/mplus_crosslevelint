USEVARIABLES ARE
L1CTRL1 !Level1 Control variables
L1CTRL2
L1CTRL3
X Y1 Y2; !IV and DV

MISSING = all(999);

DEFINE:
X=(X1+X2+X3)/3;
Y1 =(Y1_1+Y1_2+Y1_3+Y1_4+Y1_5)/5;
Y2= (Y2_1+Y2_2+Y2_3)/3;

CENTER L1CTRL1 L1CTRL2 L1CTRL3 X Y1 Y2 (grandmean); 

ANALYSIS:
TYPE=GENERAL;
ESTIMATOR=ML;

MODEL:
X Y1 Y2;
Y1 on X L1CTRL1 L1CTRL2 L1CTRL3;
Y2 on X L1CTRL1 L1CTRL2 L1CTRL3;
X on L1CTRL1 L1CTRL2 L1CTRL3;

X with Y1 Y2;
Y1 with Y2;

OUTPUT:
SAMPSTAT
CINTERVAL;

